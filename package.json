{
  "name": "hyperpuzzlescript",
  "version": "0.0.19",
  "publisher": "HactarCE",
  "engines": {
    "vscode": "^1.28.0"
  },
  "license": "MIT",
  "displayName": "Hyperpuzzlescript Language Support",
  "description": "Twisty puzzle definition syntax highlighting",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "hyperpuzzlescript",
    "hyperspeedcube",
    "hsc",
    "puzzle"
  ],
  "contributes": {
    "languages": [
      {
        "id": "hps",
        "aliases": [
          "Hyperpuzzlescript"
        ],
        "extensions": [
          ".hps"
        ],
        "configuration": "./syntax/hps.configuration.json",
        "icon": {
          "light": "./icons/hps.png",
          "dark": "./icons/hps.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "hps",
        "scopeName": "source.hps",
        "path": "./syntax/hps.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "hps",
        "path": "snippets/hps.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HactarCE/vscode-hps.git"
  },
  "icon": "icons/hps.png",
  "devDependencies": {
    "js-yaml": "^4.1.0"
  }
}
